<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
        <add key="ado-feed"
            value="%NUGET_SERVER_URL%" />
    </packageSources>
    <packageSourceCredentials>
        <ado-feed>
            <add key="Username" value="%NUGET_SERVER_USERNAME%" />
            <add key="ClearTextPassword"
                value="%NUGET_SERVER_PASSWORD%" />
        </ado-feed>
    </packageSourceCredentials>
</configuration>